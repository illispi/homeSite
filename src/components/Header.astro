---
import Logo from "./Logo.astro";
import Nav from "./Nav.astro";

const { current = "" } = Astro.props;
---

<style>
  header {
    display: flex;
    margin: 0 auto;
    max-width: 1400px;
    width: 100%;
  }
</style>

<header class="bg-zinc-900 w-screen">
  <Logo />
  <Nav current={current} />
</header>
